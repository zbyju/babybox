{
  "name": "babybox-panel",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && cp -r ./dist ../babybox-panel-backend/dist/public/",
    "update": "git restore . && git pull && pnpm install",
    "build:win": "vue-tsc --noEmit && vite build && robocopy \"./dist\" \"../babybox-panel-backend/dist/public/\" /s /e *.*;",
    "asdf": "echo test",
    "update:win": "git restore . && git pull && pnpm install",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier . --write",
    "echo": "echo 'Hello from panel'"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "howler": "^2.2.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/howler": "^2.2.7",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.27",
    "@typescript-eslint": "link:@vue/prettier/@typescript-eslint",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "prettier": "^2.7.1",
    "stylus": "^0.57.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.38.2"
  }
}
